{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "Backend para un sistema de eCommerce desarrollado con Node.js, Express, y MySQL. Gestiona la autenticación de usuarios, manejo de productos, pedidos y más.",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "start": "npx prisma migrate deploy && node src/index.js",
    "dev": "nodemon src/index.js ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": " ncu -u"
  },
  "keywords": [
    "ecommerce",
    "backend",
    "nodejs",
    "express",
    "mysql"
  ],
  "author": "Diego Suarez",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "fs-extra": "^11.2.0",
    "i18next": "^23.16.2",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-middleware": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "mysql2": "^3.11.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
